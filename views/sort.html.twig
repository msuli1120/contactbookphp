{% include 'head.html.twig' %}
  {% if contacts is not empty %}
    <h2>You sorted by group name {{ grp }}:</h2>
    {% for contact in contacts %}
      <li>Name: {{ contact.getName }}</li>
      <li>Mobile: {{ contact.getMobile }}</li>
      <li>Email: {{ contact.getEmail }}</li>
      <li>Company: {{ contact.getCompany }}</li>
      <li>Relation: {{ contact.getRelation }}</li>
      <li>Address: {{ contact.getAddress }}</li>
      <br>
    {% endfor %}
  {% endif %}
  <form action="/go_back" method="post">
    <button type="submit">Go Back</button>
  </form>
{% include 'foot.html.twig' %}
